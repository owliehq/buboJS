    "@semantic-release/npm": "^9.0.2",
    "@types/node": "^18.0.0",
    "@types/pluralize": "0.0.29",
    "rimraf": "^3.0.2",
    "semantic-release": "20.0.4",
    "supertest": "^6.2.3",
